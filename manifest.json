{
  "name": "Quick Zoom",
  "version": "1.0",
  "description": "Enables browser zoom with Cmd + mouse wheel",
  "background": {
    "scripts": ["background.js"],
    "persistant": "false"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": [],
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "manifest_version": 2
}
